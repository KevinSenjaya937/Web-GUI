@{
    @using WebGUI.Models;
    @model IEnumerable<Customer>;

}

<div id="tableview">
    <table>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.AccountNumber</td>
                <td>@customer.FirstName</td>
                <td>@customer.LastName</td>
                <td>@customer.PinNumber</td>
                <td>@customer.Balance</td>
            </tr> 
        }

    </table>
    

</div>
